<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:contracts="clr-namespace:Restless.Camera.Contracts;assembly=Restless.Camera.Contracts">
    
    <contracts:Orientation x:Key="OrientationNormal">Normal</contracts:Orientation>
    <contracts:Orientation x:Key="OrientationFlip">Flip</contracts:Orientation>
    <contracts:Orientation x:Key="OrientationMirror">Mirror</contracts:Orientation>
    <contracts:Orientation x:Key="OrientationFlipAndMirror">FlipAndMirror</contracts:Orientation>

    <contracts:FrameRate x:Key="FrameRateLow">Low</contracts:FrameRate>
    <contracts:FrameRate x:Key="FrameRateMedium">Medium</contracts:FrameRate>
    <contracts:FrameRate x:Key="FrameRateHigh">High</contracts:FrameRate>
    
</ResourceDictionary>